{
    "presets": [
        [
            "es2015", {
                "modules": false
            }
        ],
        "react",
        "stage-2"
    ],
    "plugins": [
        "transform-runtime",
        [
            "import", {
                "libraryName": "antd",
                "style": "css"
            }
        ],
        "transform-decorators-legacy"
    ],
    "env": {
        "test": {
            "plugins": [
                "transform-es2015-modules-commonjs",
                "dynamic-import-node"
            ]
        }
    }
}